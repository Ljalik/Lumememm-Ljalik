<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Lumememm LOGITpv19.lumememm">
    <ContentPage.Content>
        <StackLayout BackgroundColor="White">
            <StackLayout x:Name="snegovik"
                         BackgroundColor="Green"
                         Spacing="0">
                <Frame x:Name="shlapa"
                    BackgroundColor="Brown"
                       BorderColor="Black"
                       RotationX="90"
                       Margin="140,0"
                       Padding="45"/>
                <Frame x:Name="golova"
                       BackgroundColor="Black"
                       CornerRadius="60"
                       Margin="120,0"
                       Padding="60">
                </Frame>
                <Frame x:Name="telo"
                       BackgroundColor="Red"
                       CornerRadius="80"
                       Margin="100,0"
                       Padding="80">
                </Frame>
                <Frame x:Name="nogi"
                       BackgroundColor="Yellow"
                       CornerRadius="200"
                       Margin="75,0"
                       Padding="90">
                </Frame>
            </StackLayout>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>